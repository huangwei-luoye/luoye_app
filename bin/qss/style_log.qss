QWidget
{
	font: 12pt;
	font: "微软雅黑";
	color:#FFFFFF;
}

QPushButton
{
	min-height:30px;
	min-width:70px;
	max-height:30px;
	max-width:70px;
	border:1px solid rgb(33, 149, 243);
	border-radius:5px;
}

QPushButton::hover
{
	background-color:#2196F3;
}
QPushButton::selected
{
	border:1px solid rgb(149, 158, 204);
}
QPushButton::disabled
{
	color: #434656;
	border:1px solid rgb(67, 70, 86);
}